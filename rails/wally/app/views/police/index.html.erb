<p id="notice"><%= notice %></p>
<script src="//cdn.rawgit.com/mahnunchik/markerclustererplus/master/dist/markerclusterer.min.js"></script>
<script src='//cdn.rawgit.com/printercu/google-maps-utility-library-v3-read-only/master/infobox/src/infobox_packed.js' type='text/javascript'></script> <!-- only if you need custom infoboxes -->
<h1>Listing Police</h1>

<table>
  <thead>
    <tr>
      <th>First name</th>
      <th>Second name</th>
      <th>Father name</th>
      <th>Mother name</th>
      <th>Latitude</th>
      <th>Longitude</th>
      <th>Current Status</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @police.each do |police| %>
      <tr>
        <td><%= police.first_name %></td>
        <td><%= police.second_name %></td>
        <td><%= police.father_name %></td>
        <td><%= police.mother_name %></td>
        <td><%= police.latitude %></td>
        <td><%= police.longitude %></td>
        <td><%= police.current_status %></td>
        <td><%= link_to 'Show', police %></td>
        <td><%= link_to 'Edit', edit_police_path(police) %></td>
        <td><%= link_to 'Destroy', police, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>


<br>

<%= link_to 'New Police', new_police_path %>






